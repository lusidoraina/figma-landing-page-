<product-info
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  {% if section.settings.image_zoom == "hover" %}
    data-zoom-on-hover
  {% endif %}
>
  {{ "section-main-product.css" | asset_url | stylesheet_tag }}
  {{ "component-accordion.css" | asset_url | stylesheet_tag }}
  {{ "component-price.css" | asset_url | stylesheet_tag }}
  {{ "component-slider.css" | asset_url | stylesheet_tag }}
  {{ "component-rating.css" | asset_url | stylesheet_tag }}
  {{ "component-deferred-media.css" | asset_url | stylesheet_tag }}

  {% unless product.has_only_default_variant %}
    {{ "component-product-variant-picker.css" | asset_url | stylesheet_tag }}
    {{ "component-swatch-input.css" | asset_url | stylesheet_tag }}
    {{ "component-swatch.css" | asset_url | stylesheet_tag }}
  {% endunless %}
  {%- if product.quantity_price_breaks_configured? -%}
    {{ "component-volume-pricing.css" | asset_url | stylesheet_tag }}
  {%- endif -%}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.mobile_padding_top }}px;
      padding-bottom: {{ section.settings.mobile_padding_bottom }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% if section.settings.image_zoom == "hover" %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
  {%- if first_3d_model -%}
    {{ "component-product-model.css" | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: "attached_to_variant?", true | map: "src" %}
  <div class="page-width">
    <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
      <div class="grid__item product__media-wrapper">
        {% render "product-media-gallery",
          variant_images: variant_images,
          image_rounded_corners: section.settings.image_rounded_corners
        %}
      </div>
      <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
        <section
          id="ProductInfo-{{ section.id }}"
          class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
        >
          {%- assign product_form_id = "product-form-" | append: section.id -%}

          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when "@app" -%}
                {% render block %}
              {%- when "text" -%}
                <p
                  class="product__text inline-richtext{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                  {{ block.shopify_attributes }}
                >
                  {{- block.settings.text -}}
                </p>

              {%- when "product-title" -%}
                {% assign product_title_section_padding = "padding-top: "
                  | append: block.settings.product_title_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.product_title_padding_bottom
                  | append: "px;"
                %}

                <div class="pagepilot-layout" style="{{ product_title_section_padding }}">
                  <h2
                    class="{{ block.settings.product_title_heading_size }} pp-text-{{ block.settings.product_title_heading_mobilealignment }} sm:pp-text-{{ block.settings.product_title_heading_alignment }}"
                    style="color: {{ block.settings.product_title_heading_color }};"
                  >
                    {{ product.title }}
                  </h2>
                </div>

              {%- when "pp-text" -%}
                {% assign pp_text_padding = "padding-top: "
                  | append: block.settings.pp_text_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_text_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_text_border = "" %}
                {% if block.settings.pp_text_border_top > 0 %}
                  {% assign pp_text_border = pp_text_border
                    | append: "border-top: "
                    | append: block.settings.pp_text_border_top
                    | append: "px solid "
                    | append: block.settings.pp_text_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_text_border_bottom > 0 %}
                  {% assign pp_text_border = pp_text_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_text_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_text_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_text_padding }} {{ pp_text_border }}">
                  <div
                    class="{{ block.settings.pp_text_size }} pp-text-{{ block.settings.pp_text_mobilealignment }} sm:pp-text-{{ block.settings.pp_text_alignment }}"
                    style="color: {{ block.settings.pp_text_color }};"
                    {{ block.shopify_attributes }}
                  >
                    {{ block.settings.pp_text }}
                  </div>
                </div>

              {%- when "pp-expandable" -%}
                {% assign pp_expandable_padding = "padding-top: "
                  | append: block.settings.pp_expandable_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_expandable_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_expandable_border = "" %}
                {% if block.settings.pp_expandable_border_top > 0 %}
                  {% assign pp_expandable_border = pp_expandable_border
                    | append: "border-top: "
                    | append: block.settings.pp_expandable_border_top
                    | append: "px solid "
                    | append: block.settings.pp_expandable_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_expandable_border_bottom > 0 %}
                  {% assign pp_expandable_border = pp_expandable_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_expandable_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_expandable_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_expandable_padding }}{{ pp_expandable_border }}">
                  <div class="pp-flex pp-flex-col pp-gap-6">
                    {% for i in (1..3) %}
                      {% assign title_key = "pp_expandable_title" | append: i %}
                      {% assign content_key = "pp_expandable_content" | append: i %}
                      {% if block.settings[title_key] != blank and block.settings[content_key] != blank %}
                        <details
                          class="pp-group pp-appearance-none pp-outline-none pp-transition pp-transition-transform pp-duration-500 hover:pp-cursor-pointer"
                          style="border-bottom: 1px solid {{ block.settings.pp_expandable_text_border_color }}; padding-bottom: 8px;"
                        >
                          <summary class="pp-flex pp-w-full pp-appearance-none pp-items-center pp-justify-between pp-py-2 pp-relative">
                            <div
                              class="{% if block.settings.pp_expandable_text_size == 'small' %}pp-text-xl{% elsif block.settings.pp_expandable_text_size == 'medium' %}body{% elsif block.settings.pp_expandable_text_size == 'large' %}subtitle{% endif %}"
                              style="color: {{ block.settings.pp_expandable_text_color }};"
                            >
                              {{ block.settings[title_key] }}
                            </div>

                            <svg
                              class="pp-absolute pp-right-0 pp-top-0 pp-transition-transform pp-duration-500 group-open:pp-rotate-180"
                              xmlns="http://www.w3.org/2000/svg"
                              viewBox="0 0 256 256"
                              style="width: 16px; height: 16px; fill: {{ block.settings.pp_expandable_arrow_color }};"
                            >
                              <path d="M216.49,104.49l-80,80a12,12,0,0,1-17,0l-80-80a12,12,0,0,1,17-17L128,159l71.51-71.52a12,12,0,0,1,17,17Z"></path>
                            </svg>
                          </summary>
                          <article class="pp-overflow-hidden" style="padding-top: 8px; padding-bottom: 8px;">
                            <p
                              class="{% if block.settings.pp_expandable_answer_size == 'small' %}pp-text-xl{% elsif block.settings.pp_expandable_answer_size == 'medium' %}body{% elsif block.settings.pp_expandable_answer_size == 'large' %}subtitle{% endif %}"
                              style="color: {{ block.settings.pp_expandable_text_color }};"
                            >
                              {{ block.settings[content_key] }}
                            </p>
                          </article>
                        </details>
                      {% endif %}
                    {% endfor %}
                  </div>
                </div>

              {%- when "pp-partners" -%}
                {% assign pp_partners_padding = "padding-top: "
                  | append: block.settings.pp_partners_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_partners_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_partners_border = "" %}
                {% if block.settings.pp_partners_border_top > 0 %}
                  {% assign pp_partners_border = pp_partners_border
                    | append: "border-top: "
                    | append: block.settings.pp_partners_border_top
                    | append: "px solid "
                    | append: block.settings.pp_partners_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_partners_border_bottom > 0 %}
                  {% assign pp_partners_border = pp_partners_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_partners_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_partners_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_partners_padding }}{{ pp_partners_border }}">
                  <div class="pp-w-full pp-max-w-[650px] pp-relative">
                    <div
                      class="pp-w-full pp-flex pp-items-center pp-justify-center pp-px-20 pp-absolute pp-top-0 pp-right-0 pp-left-0 pp-w-[50%] pp-h-full pp-z-10 pp-mx-auto"
                      style="background: {{ block.settings.pp_partners_gradient }};"
                    >
                      <div
                        class="{% if block.settings.pp_partners_heading_size == 'small' %}pp-text-xl{% elsif block.settings.pp_partners_heading_size == 'medium' %}body{% elsif block.settings.pp_partners_heading_size == 'large' %}subtitle{% endif %} pp-text-center"
                        style="color: {{ block.settings.pp_partners_heading_color }};"
                      >
                        {{ block.settings.pp_partners_heading }}
                      </div>
                    </div>
                    <div class="pp-relative pp-flex pp-overflow-hidden pp-gap-2">
                      <div class="pp-flex pp-items-center pp-justify-around pp-flex-shrink-0 pp-min-w-full pp-gap-4 pp-animate-partnersCarousel">
                        {% for i in (1..6) %}
                          {% assign image_key = "pp_partners_image" | append: i %}
                          {% if block.settings[image_key] != blank %}
                            <img
                              class="pp-w-full pp-max-w-[100px] pp-h-fit pp-grayscale"
                              src="{{ block.settings[image_key] | img_url: '400x' }}"
                              alt="{{ block.settings[image_key].alt }}"
                              width="{{ block.settings[image_key].width }}"
                              height="{{ block.settings[image_key].height }}"
                            >
                          {% else %}
                            <img
                              class="pp-w-full pp-max-w-[100px] pp-h-fit"
                              src="{{ 'partners' | append: i | append: '.png' | asset_url }}"
                              alt="Partner {{ i }}"
                              width="100"
                              height="100"
                            >
                          {% endif %}
                        {% endfor %}
                      </div>
                      <div class="pp-flex pp-items-center  pp-justify-around pp-flex-shrink-0 pp-min-w-full pp-gap-4 pp-animate-partnersCarousel">
                        {% for i in (1..6) %}
                          {% assign image_key = "pp_partners_image" | append: i %}
                          {% if block.settings[image_key] != blank %}
                            <img
                              class="pp-w-full pp-max-w-[100px] pp-h-fit pp-grayscale"
                              src="{{ block.settings[image_key] | img_url: '400x' }}"
                              alt="{{ block.settings[image_key].alt }}"
                              width="{{ block.settings[image_key].width }}"
                              height="{{ block.settings[image_key].height }}"
                            >
                          {% else %}
                            <img
                              class="pp-w-full pp-max-w-[100px] pp-h-fit"
                              src="{{ 'partners' | append: i | append: '.png' | asset_url }}"
                              alt="Partner {{ i }}"
                              width="100"
                              height="100"
                            >
                          {% endif %}
                        {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>

              {%- when "pp-benefits" -%}
                {% assign pp_benefits_padding = "padding-top: "
                  | append: block.settings.pp_benefits_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_benefits_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_benefits_border = "" %}
                {% if block.settings.pp_benefits_border_top > 0 %}
                  {% assign pp_benefits_border = pp_benefits_border
                    | append: "border-top: "
                    | append: block.settings.pp_benefits_border_top
                    | append: "px solid "
                    | append: block.settings.pp_benefits_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_benefits_border_bottom > 0 %}
                  {% assign pp_benefits_border = pp_benefits_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_benefits_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_benefits_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_benefits_padding }}{{ pp_benefits_border }}">
                  <div class="pp-flex pp-flex-col pp-gap-2">
                    {% for i in (1..4) %}
                      {% assign text_key = "pp_benefits_text" | append: i %}
                      {% if block.settings[text_key] != blank %}
                        <div
                          class="{{ block.settings.pp_benefits_text_size }} pp-text-{{ block.settings.pp_benefits_text_mobilealignment }} sm:pp-text-{{ block.settings.pp_benefits_text_alignment }}"
                          style="color: {{ block.settings.pp_benefits_text_color }};"
                          {{ block.shopify_attributes }}
                        >
                          {{ block.settings[text_key] }}
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                </div>

              {%- when "pp-review" -%}
                {% assign pp_review_section_padding = "padding-top: "
                  | append: block.settings.pp_review_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_review_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_review_border = "" %}
                {% if block.settings.pp_review_border_top > 0 %}
                  {% assign pp_review_border = pp_review_border
                    | append: "border-top: "
                    | append: block.settings.pp_review_border_top
                    | append: "px solid "
                    | append: block.settings.pp_review_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_review_border_bottom > 0 %}
                  {% assign pp_review_border = pp_review_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_review_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_review_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_review_section_padding }} {{ pp_review_border }}">
                  <div class="pp-flex pp-gap-6 pp-items-center">
                    {% if block.settings.pp_review_image != blank %}
                      {% assign pp_review_image_border_radius_class = "" %}
                      {% case block.settings.pp_review_image_border_radius %}
                        {% when "small" %}
                          {% assign pp_review_image_border_radius_class = "pp-rounded-2xl" %}
                        {% when "large" %}
                          {% assign pp_review_image_border_radius_class = "pp-rounded-3xl" %}
                        {% when "full" %}
                          {% assign pp_review_image_border_radius_class = "pp-rounded-full" %}
                      {% endcase %}

                      <img
                        src="{{ block.settings.pp_review_image | img_url: 'master' }}"
                        alt="{{ block.settings.pp_review_image.alt | escape }}"
                        loading="lazy"
                        class="pp-w-full {{ pp_review_image_border_radius_class }}"
                        width="{{ block.settings.pp_review_image.width }}"
                        height="{{ block.settings.pp_review_image.height }}"
                        style="max-width: 85px;"
                      >
                    {% endif %}

                    <div class="pp-flex pp-flex-col pp-gap-2">
                      {% assign pp_review_stars_size = 16 %}

                      {% case block.settings.pp_review_stars_size %}
                        {% when "sm" %}
                          {% assign pp_review_stars_size = 16 %}
                        {% when "md" %}
                          {% assign pp_review_stars_size = 18 %}
                        {% when "lg" %}
                          {% assign pp_review_stars_size = 24 %}
                      {% endcase %}

                      {% if block.settings.pp_review_stars_number > 0 %}
                        <div class="pp-flex pp-gap-0.5 pp-justify-{{ block.settings.pp_review_stars_mobilealignment }} sm:pp-justify-{{ block.settings.pp_review_stars_alignment }}">
                          {% for i in (1..block.settings.pp_review_stars_number) %}
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              viewBox="0 0 256 256"
                              style="width: {{ pp_review_stars_size }}px; height: {{ pp_review_stars_size }}px; fill: {{ block.settings.pp_review_stars_color }}; "
                            >
                              <path d="M234.29,114.85l-45,38.83L203,211.75a16.4,16.4,0,0,1-24.5,17.82L128,198.49,77.47,229.57A16.4,16.4,0,0,1,53,211.75l13.76-58.07-45-38.83A16.46,16.46,0,0,1,31.08,86l59-4.76,22.76-55.08a16.36,16.36,0,0,1,30.27,0l22.75,55.08,59,4.76a16.46,16.46,0,0,1,9.37,28.86Z"></path>
                            </svg>
                          {% endfor %}
                        </div>
                      {% endif %}

                      <div
                        class="{% if block.settings.pp_review_text_style == 'small' %}pp-text-xl{% elsif block.settings.pp_review_text_style == 'medium' %}body{% elsif block.settings.pp_review_text_style == 'large' %}subtitle{% endif %} pp-text-{{ block.settings.pp_review_text_mobilealignment }} sm:pp-text-{{ block.settings.pp_review_text_alignment }}"
                        style="color: {{ block.settings.pp_review_text_color }};"
                      >
                        {{ block.settings.pp_review_text }}
                      </div>

                      {% if block.settings.pp_review_author_badge_text != blank %}
                        <div
                          class="pp-flex pp-gap-2 pp-items-center pp-font-medium pp-mt-2"
                          style="color: {{ block.settings.pp_review_author_badge_text_color }};"
                        >
                          <svg
                            xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 256 256"
                            style="{% if block.settings.pp_review_author_badge_text_style == 'small' %}width: 16px; height: 16px;{% elsif block.settings.pp_review_author_badge_text_style == 'medium' %}width: 18px; height: 18px;{% else %}width: 21px; height: 21px;{% endif %} fill: {{ block.settings.pp_review_author_badge_color }};"
                          >
                            <path d="M128,24A104,104,0,1,0,232,128,104.11,104.11,0,0,0,128,24Zm45.66,85.66-56,56a8,8,0,0,1-11.32,0l-24-24a8,8,0,0,1,11.32-11.32L112,148.69l50.34-50.35a8,8,0,0,1,11.32,11.32Z"></path>
                          </svg>
                          <div class="{% if block.settings.pp_review_author_badge_text_style == 'small' %}pp-text-xl{% elsif block.settings.pp_review_author_badge_text_style == 'medium' %}body{% elsif block.settings.pp_review_author_badge_text_style == 'large' %}subtitle{% endif %}">
                            {{ block.settings.pp_review_author_badge_text }}
                          </div>
                        </div>
                      {% endif %}
                    </div>
                  </div>
                </div>

              {%- when "pp-guarantees" -%}
                {% assign pp_guarantees_padding = "padding-top: "
                  | append: block.settings.pp_guarantees_padding_top
                  | append: "px; padding-bottom: "
                  | append: block.settings.pp_guarantees_padding_bottom
                  | append: "px;"
                %}

                {% assign pp_guarantees_border = "" %}
                {% if block.settings.pp_guarantees_border_top > 0 %}
                  {% assign pp_guarantees_border = pp_guarantees_border
                    | append: "border-top: "
                    | append: block.settings.pp_guarantees_border_top
                    | append: "px solid "
                    | append: block.settings.pp_guarantees_border_color
                    | append: ";"
                  %}
                {% endif %}
                {% if block.settings.pp_guarantees_border_bottom > 0 %}
                  {% assign pp_guarantees_border = pp_guarantees_border
                    | append: "border-bottom: "
                    | append: block.settings.pp_guarantees_border_bottom
                    | append: "px solid "
                    | append: block.settings.pp_guarantees_border_color
                    | append: ";"
                  %}
                {% endif %}

                <div class="pagepilot-layout" style="{{ pp_guarantees_padding }}{{ pp_guarantees_border }}">
                  <div class="pp-grid pp-grid-cols-3 pp-gap-4 pp-items-start">
                    {% for i in (1..3) %}
                      {% assign icon_key = "pp_guarantees_icon" | append: i %}
                      {% assign text_key = "pp_guarantees_text" | append: i %}
                      {% if block.settings[text_key] != blank %}
                        <div class="pp-flex pp-flex-col pp-items-center pp-justify-center pp-gap-4">
                          {% assign icon_key = "pp_guarantees_icon" | append: i %}
                          <div class="pp-w-full pp-flex pp-justify-center">
                            {% case block.settings[icon_key] %}
                              {% when "back" %}
                                <svg
                                  class="pp-w-full"
                                  viewBox="0 0 30 30"
                                  xmlns="http://www.w3.org/2000/svg"
